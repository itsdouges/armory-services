{
  "name": "armory-back",
  "version": "1.0.0",
  "description": "Backend services for gw2armory",
  "main": "index.js",
  "scripts": {
    "start": "node ./dist",
    "dev": "ENV=DEV VERSION=local ./go.sh build && ./go.sh dev",
    "fetch": "node ./dist/fetch",
    "api": "node ./dist/api -s",
    "migration": "sequelize db:migrate",
    "deploy": "node ./scripts/deploy",
    "build": "babel src --out-dir dist --ignore spec.js,int.js",
    "test": "npm run flow && mocha \"src/**/*.spec.js\" && npm run test:int",
    "test:int": "mocha \"src/**/*.int.js\"",
    "tdd": "mocha \"src/**/*.spec.js\" --watch",
    "tdd:int": "npm run test:int -- --watch",
    "lint": "eslint .",
    "flow": "flow",
    "flowtypes": "flow-typed install --overwrite",
    "clean-docker": "./scripts/go.sh clean"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/madou/armory-back.git"
  },
  "author": "madou",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/madou/armory-back/issues"
  },
  "homepage": "https://api.gw2armory.com",
  "dependencies": {
    "lodash": "^4.16.4",
    "lodash.assign": "^4.2.0",
    "mysql": "^2.8.0",
    "sequelize": "^3.6.0",
    "sequelize-cli": "^2.4.0",
    "yargs": "^3.29.0",
    "aws-sdk": "^2.5.3",
    "axios": "^0.5.4",
    "babel-polyfill": "^6.20.0",
    "fs": "0.0.2",
    "gotta-validate": "^0.2.x",
    "memoizee": "^0.4.1",
    "moment": "^2.14.1",
    "nodemailer": "^2.6.0",
    "nodemailer-ses-transport": "^1.4.0",
    "password-hash-and-salt": "^0.1.2",
    "path": "^0.11.14",
    "restify": "^3.0.3",
    "restify-oauth2": "^4.0.3",
    "lodash": "^4.16.4",
    "node-gitter": "^2.0.5",
    "q": "^1.4.1",
    "throat": "^2.0.2"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-syntax-flow": "^6.18.0",
    "babel-plugin-transform-flow-strip-types": "^6.18.0",
    "babel-register": "^6.18.0",
    "beanstalkify": "0.0.9",
    "easy-zip": "0.0.4",
    "eslint": "^3.4.0",
    "eslint-config-airbnb": "^10.0.1",
    "eslint-plugin-flowtype": "^2.29.1",
    "eslint-plugin-import": "^1.14.0",
    "eslint-plugin-jsx-a11y": "^2.2.0",
    "flow-bin": "^0.36.0",
    "flow-typed": "^2.0.0",
    "hash-files": "^1.0.0",
    "mustache": "^2.2.0",
    "app-module-path": "^2.1.0",
    "babel-plugin-transform-object-rest-spread": "^6.20.2",
    "babel-preset-latest": "^6.16.0",
    "chai": "^3.5.0",
    "chai-as-promised": "^6.0.0",
    "mocha": "^3.1.1",
    "nodemon": "^1.4.1",
    "run-sequence": "^1.1.2",
    "sinon": "^1.17.6",
    "sinon-chai": "^2.8.0",
    "sqlite3": "^3.0.10",
    "proxyquire": "^1.7.10"
  }
}
